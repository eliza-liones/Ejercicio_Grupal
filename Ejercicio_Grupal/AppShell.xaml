    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Ejercicio_Grupal"
    Shell.FlyoutBehavior="Flyout"
    Title="Ejercicio_Grupal"
    BackgroundColor="#CBAACB">

    <TabBar>
        <ShellContent
         Title="Ejer.1"
